<Report>
	<Font name="Arial" size="7" style="" id="ArialSmall"/>
	<Font name="Arial" size="17" style="B" id="ArialBoldBigB" red="0" green="0" blue="128"/>
	<Font name="Arial" size="8" style="B" id="ArialBold"/>
	<Font name="Arial" size="8" style="B" id="ArialBold_header"/>	
	<Font name="Arial" size="10" style="" id="ArialSmallBlue" red="0" green="0" blue="128"/>
	<Font name="Arial" size="9" style="" id="Arial"/>
	<PageHeader>	
		<Cell text="" height="55%" newline="1"/>		
		<Cell text="PAG. {SectionPageNumber}/{snb}" align="R" font="ArialSmall" newline="1" />
		<Cell text="INFORME BACKCHARGE" width="100%"  align="C" newline="1" font="ArialBoldBigB"/>
		<Cell text="{str_filtro}" width="100%" align="C" newline="1" font="ArialSmallBlue"/>
		<Cell text="" height="4" newline="1"/>
		<Cell text="" height="4" newline="1"/>
		<Cell text="" height="4" newline="1"/>	
	</PageHeader>
	<Content font="Arial">
		<Cell text="{COD_NOTA_VENTA}" width="8%" align="L" border="LRTB" font="Arial"/>	
		<Cell text="{FECHA_NOTA_VENTA}" width="8%" align="L" border="LRTB" font="Arial"/>	
		<Cell text="{NOM_USUARIO}" width="12%" align="C" border="LRTB" font="Arial"/>
		<Cell text="{TOTAL_NETO}" width="8%" align="R" border="LRTB" format="N0,." font="Arial"/>	
		<Cell text="{VENTA_NETA}" width="8%" align="R" border="LRTB" format="N0,." font="Arial"/>
		<Cell text="{RESULTADO}" width="8%" align="R" border="LRTB" format="N0,." font="Arial" />
		<Cell text="{PORC_RESULTADO}" width="8%" align="R" border="LRTB" format="N1,." font="Arial" />
		<Cell text="{PORC_VENDEDOR1}" width="8%" align="R" border="LRTB" format="N1,." font="Arial" />
		<Cell text="{COMISION_V1}" width="8%" align="R" border="LRTB" format="N0,." font="Arial" />
		<Cell text="{BACKCHARGE}" width="8%" align="R" border="LRTB" format="N0,." font="Arial" />
		<Cell text="{COMISION_V1_BACKCHARGE}" width="8%" align="R" border="LRTB" format="N0,." font="Arial" />
		<Cell text="{DIF}" width="8%" align="R" border="LRTB" format="N0,." font="Arial" newline="1"/>
	</Content>
	<Group>
		<GroupHeader>
			<Cell text="N° NV" width="8%" align="C" border="LRTB" font="ArialSmallBlue"/>		
			<Cell text="Fecha NV" width="8%" align="C" border="LRTB" font="ArialSmallBlue"/>		
			<Cell text="Usuario" width="12%" align="C" border="LRTB" font="ArialSmallBlue"/>
			<Cell text="Tot. Neto" width="8%" align="C" border="LRTB" font="ArialSmallBlue"/>
			<Cell text="Venta Neta" width="8%" align="C" border="LRTB" font="ArialSmallBlue"/>
			<Cell text="Resultado" width="8%" align="C" border="LRTB" font="ArialSmallBlue"/>
			<Cell text="% Resultado" width="8%" align="C" border="LRTB" font="ArialSmallBlue"/>
			<Cell text="% Vendedor" width="8%" align="C" border="LRTB" font="ArialSmallBlue"/>
			<Cell text="Comisión" width="8%" align="C" border="LRTB" font="ArialSmallBlue"/>
			<Cell text="Backcharge" width="8%" align="C" border="LRTB" font="ArialSmallBlue"/>
			<Cell text="Comisión Backcharge" width="8%" align="C" border="LRTB" font="ArialSmallBlue"/>
			<Cell text="Diferencia" width="8%" align="C" border="LRTB" font="ArialSmallBlue" newline="1"/>
		</GroupHeader>
	</Group>
</Report>