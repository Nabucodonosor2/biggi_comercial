								<td width="15%">
									<div align="center"><strong>Fecha Compromiso</strong></div>
								</td>
							</tr>
							 <!-- startBlock(BITACORA_COTIZACION_HOY) -->
							<tr class="{DW_TR_CSS}" id="{DW_TR_ID}">
